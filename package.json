{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "description": "test coverage for [cors-anywhere (v0.4.0)](https://github.com/Rob--W/cors-anywhere#readme) [![npm package](https://img.shields.io/npm/v/npmtest-cors-anywhere.svg?style=flat-square)](https://www.npmjs.org/package/npmtest-cors-anywhere) [![travis-ci.org build-status](https://api.travis-ci.org/npmtest/node-npmtest-cors-anywhere.svg)](https://travis-ci.org/npmtest/node-npmtest-cors-anywhere)",
    "devDependencies": {
        "cors-anywhere": "*",
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmtest/node-npmtest-cors-anywhere",
    "keywords": [
        "coverage",
        "test",
        "cors-anywhere"
    ],
    "license": "MIT",
    "main": "lib.npmtest_cors_anywhere.js",
    "name": "npmtest-cors-anywhere",
    "nameAlias": "npmtest_cors_anywhere",
    "nameOriginal": "npmtest-cors-anywhere",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmtest/node-npmtest-cors-anywhere.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "npm install \"kaizhu256/node-utility2#alpha\" && utility2 shDeployHeroku",
        "postinstall": "[ ! -f npm_scripts.sh ] || ./npm_scripts.sh postinstall",
        "start": "PORT=${PORT:-8080} utility2 start test.js",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.1"
}
